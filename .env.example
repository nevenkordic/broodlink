# Broodlink Environment Variables
# Copy to .env and fill in values for your deployment.
#
# The config file path (required)
BROODLINK_CONFIG=./config.toml

# Database passwords (referenced by config password_key fields)
BROODLINK_DOLT_PASSWORD=
BROODLINK_POSTGRES_PASSWORD=

# Qdrant API key (leave blank for local dev without auth)
BROODLINK_QDRANT_API_KEY=

# Status API key for dashboard and inter-service auth
BROODLINK_STATUS_API_KEY=dev-api-key

# A2A Gateway API key
BROODLINK_A2A_API_KEY=

# Slack webhook signature verification (a2a-gateway)
# SLACK_SIGNING_SECRET=

# Telegram bot secret token (a2a-gateway)
# TELEGRAM_SECRET_TOKEN=

# OpenTelemetry (if telemetry.enabled = true)
# OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317

# Infisical (if secrets.provider = "infisical")
# INFISICAL_TOKEN=
